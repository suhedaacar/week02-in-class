  
  def size(list: List[Int]): Int = {
    list match{
      case Nil => 0
      case _::tail => 1 + size(tail)
    }
  }

  def last(list: List[Int]): Int = {
    list match {
      case Nil => 0
      case h::Nil => h
      case _::tail => last(tail)
    }
  }

  def reverse(list: List[Int]): Int = {
    list match {
      case Nil => Nil
      case h::Nil => h
      case h::t => reverse(t)::h
    }
  }
